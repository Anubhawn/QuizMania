<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:fbutton="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"

    android:gravity="center"
    android:background="#D3FFCE"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"

    tools:context="quizgm.c443.umb.edu.quizmania.ReturnHome">
<TextView
    android:id="@+id/tQ"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:text="@string/app_name"
    android:gravity="center"
    android:textColor="#35885C"
    android:textSize="50sp"/>
<View
    android:layout_width="match_parent"
    android:layout_height="20dp"></View>
<ImageView

    android:src="@drawable/gt"
    android:layout_marginBottom="10dp"
    android:layout_width="200dp"
    android:layout_height="200dp"/>

    <ImageButton

        android:id="@+id/strt"
        fbutton:srcCompat="@drawable/start"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:background="@null"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="15dp"></View>
    <ImageButton

        android:id="@+id/qit"
        android:layout_marginTop="5dp"
        android:background="@null"
        fbutton:srcCompat="@drawable/qtt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>






</LinearLayout>
